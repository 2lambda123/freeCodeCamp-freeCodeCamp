// Searching Linearly And Printing The Position Of the Element//

#include <stdio.h> //Taking Standard Input-Output//
main()
{
int a[20],n,i,j,s; // Taking maximum Size of Array as 20 elements //
printf("Enter Size of Array (less than 20)\n");
scanf("%d",&n); //Taking Size Of Array From the user
printf("Enter value of Elements of Array\n);
for (i=0;i<n;i++);
{                      // Taking Value For Each Element //
scanf("%d",&a[i]);
}
printf("Enter Value To be Searched");
scanf("%d",&s);        //Asking For Values To be Searched//
for (i=0;i<=n;i++);
{
if (s==a[i])           // Applying Search Condition//
printf("Match Found!! Its Position is",i+1);
}
if(i==n);
printf("Match Not Found");
}
