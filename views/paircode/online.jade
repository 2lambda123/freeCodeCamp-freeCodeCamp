script.
	var userOnline = !{JSON.stringify(onlineUsers)};
	if (userOnline.length === 0){
		var div = document.createElement('div');
		$(div)
		.html("<div class='story-headline col-xs-12'><center>No one is online</center></div>");
		$(div).addClass('story-list')
		$(div).appendTo($('#story-list'));
	} else {
		for (var i=0; i < userOnline.length; i++) {
			var wentOnline = new Date(userOnline[i].timeOnline).getTime();
			var onlineForMinutes = Math.round((new Date().getTime() - wentOnline)/60000);
			var minutesText = (onlineForMinutes===1)? "":"s";
			var div = document.createElement('div');
				$(div)
				.html(
				"<div class='col-xs-12 text-left'>" +
					"<h2 class='col-xs-1 col-sm-1 positive-5'>" +
						(i + 1) +
					"</h2>" +
					"<div class='col-xs-2 col-sm-1'>" +
						"<a href='/" + userOnline[i].user.email + "'>" +
							"<img src='" + userOnline[i].user.profile.picture + "' class='img-news'/>" +
						"</a>" +
					"</div>" +
					"<div class='col-xs-9 col-sm-10'>" +
						"<div class='row'>" +
							"<div class='story-headline col-xs-12'>" +
								"<a href='#'>" +
									userOnline[i].comment +
								"</a>" +
							"</div>" +
							"<div class='col-xs-12'>" +
								"<span>" +
									"posted " + onlineForMinutes + " minute" + minutesText + " ago by <a href='/" + userOnline[i].user.profile.username + "'>" + userOnline[i].user.profile.username + "</a> " +
								"</span>" +
							"</div>" +
						"</div>" +
					"</div>" +
				"</div>"
			);
			$(div).addClass('story-list')
			$(div).appendTo($('#story-list'));
		}
	}
 