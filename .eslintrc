{
    "env": {
        "browser": false,
        "es6": false,
        "node": false
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "arrowFunctions": false,
            "binaryLiterals": false,
            "blockBindings": false,
            "classes": false,
            "defaultParams": false,
            "destructuring": false,
            "forOf": false,
            "generators": false,
            "modules": false,
            "objectLiteralComputedProperties": false,
            "objectLiteralDuplicateProperties": false,
            "objectLiteralShorthandMethods": false,
            "objectLiteralShorthandProperties": false,
            "octalLiterals": false,
            "regexUFlag": false,
            "regexYFlag": false,
            "spread": false,
            "superInFunctions": false,
            "templateStrings": false,
            "unicodeCodePointEscapes": false,
            "globalReturn": false,
            "jsx": false
        },
        "sourceType": "module",
        "allowImportExportEverywhere": false
    },
    "extends": ["prettier", "prettier/react", "prettier/standard"],
    "plugins": ["react", "prettier"],
    "rules": {
        "comma-dangle": [0, "never"],
        "computed-property-spacing": [0, "never"],
        "no-cond-assign": 0,
        "no-console": 0,
        "no-constant-condition": 0,
        "no-control-regex": 0,
        "no-debugger": 0,
        "no-dupe-keys": 0,
        "no-empty": 0,
        "no-empty-character-class": 0,
        "no-ex-assign": 0,
        "no-extra-boolean-cast": 0,
        "no-extra-parens": 0,
        "no-extra-semi": 0,
        "no-invalid-this": 0,
        "no-func-assign": 0,
        "no-inner-declarations": 0,
        "no-invalid-regexp": 0,
        "no-case-declarations": 0,
        "no-irregular-whitespace": 0,
        "no-negated-in-lhs": 0,
        "no-obj-calls": 0,
        "no-regex-spaces": 0,
        "no-reserved-keys": 0,
        "no-sparse-arrays": 0,
        "no-unreachable": 0,
        "use-isnan": 0,
        "valid-jsdoc": 0,
        "valid-typeof": 0,
        "block-scoped-var": 0,
        "complexity": 0,
        "consistent-return": 0,
        "curly": 0,
        "default-case": 0,
        "dot-notation": 0,
        "eqeqeq": 0,
        "guard-for-in": 0,
        "no-alert": 0,
        "no-caller": 0,
        "no-confusing-arrow": 0,
        "no-div-regex": 0,
        "no-else-return": 0,
        "no-eq-null": 0,
        "no-eval": 0,
        "no-extend-native": 0,
        "no-extra-bind": 0,
        "no-fallthrough": 0,
        "no-floating-decimal": 0,
        "no-implied-eval": 0,
        "no-iterator": 0,
        "no-labels": 0,
        "no-lone-blocks": 0,
        "no-loop-func": 0,
        "no-multi-spaces": 0,
        "no-multi-str": 0,
        "no-native-reassign": 0,
        "no-new": 0,
        "no-new-func": 0,
        "no-new-wrappers": 0,
        "no-octal": 0,
        "no-octal-escape": 0,
        "no-process-env": 0,
        "no-proto": 0,
        "no-redeclare": 0,
        "no-return-assign": 0,
        "no-script-url": 0,
        "no-self-compare": 0,
        "no-sequences": 0,
        "no-unused-expressions": 0,
        "no-void": 0,
        "no-warning-comments": 0,
        "no-with": 0,
        "prefer-arrow-callback": 0,
        "radix": 0,
        "vars-on-top": 0,
        "wrap-iife": 0,
        "yoda": 0,
        "strict": [0, "global"],
        "prefer-const": 0,
        "no-catch-shadow": 0,
        "no-const-assign": 0,
        "no-delete-var": 0,
        "no-label-var": 0,
        "no-shadow": 0,
        "no-shadow-restricted-names": 0,
        "no-undef": 0,
        "no-undef-init": 0,
        "no-undefined": 0,
        "no-unused-vars": 0,
        "no-use-before-define": 0,
        "no-var": 0,
        "arrow-body-style": [0, "as-needed"],
        "arrow-parens": [0, "as-needed"],
        "arrow-spacing": 0,
        "brace-style": 0,
        "camelcase": 0,
        "comma-spacing": 0,
        "comma-style": 0,
        "consistent-this": 0,
        "eol-last": 0,
        "func-names": 0,
        "func-style": 0,
        "key-spacing": [
            0,
            {
                "beforeColon": false,
                "afterColon": false
            }
        ],
        "max-nested-callbacks": 0,
        "new-cap": 0,
        "new-parens": 0,
        "no-array-constructor": 0,
        "no-inline-comments": 0,
        "no-lonely-if": 0,
        "no-mixed-spaces-and-tabs": 0,
        "no-nested-ternary": 0,
        "no-new-object": 0,
        "semi-spacing": [
            0,
            {
                "before": false,
                "after": false
            }
        ],
        "no-spaced-func": 0,
        "no-ternary": 0,
        "no-trailing-spaces": 0,
        "no-multiple-empty-lines": 0,
        "no-underscore-dangle": 0,
        "one-var": 0,
        "operator-assignment": [0, "always"],
        "padded-blocks": [
            0,
            {
                "blocks": "never",
                "classes": "never",
                "switches": "never"
            }
        ],
        "quotes": [0, "double"],
        "quote-props": [0, "as-needed"],
        "semi": [0, "always"],
        "sort-vars": [
            0,
            {
                "ignoreCase": false
            }
        ],
        "keyword-spacing": 0,
        "space-before-blocks": 0,
        "object-curly-spacing": [0, "never"],
        "array-bracket-spacing": [0, "never"],
        "space-in-parens": 0,
        "space-infix-ops": 0,
        "space-unary-ops": 0,
        "spaced-comment": 0,
        "wrap-regex": 0,
        "max-depth": 0,
        "max-len": [
            0,
            120,
            {
                "ignoreStrings": false,
                "ignoreTemplateLiterals": false,
                "ignoreComments": false
            }
        ],
        "max-params": 0,
        "max-statements": 0,
        "no-plusplus": 0,
        "no-prototype-builtins": 0,
        "prefer-template": 0,
        "template-curly-spacing": [0, "never"],
        "react/display-name": 0,
        "react/jsx-no-undef": 0,
        "react/jsx-sort-props": 0,
        "react/jsx-uses-react": 0,
        "react/jsx-uses-vars": 0,
        "react/no-did-mount-set-state": 0,
        "react/no-did-update-set-state": 0,
        "react/no-multi-comp": 0,
        "react/no-unknown-property": 0,
        "react/prop-types": 0,
        "react/react-in-jsx-scope": 0,
        "react/self-closing-comp": 0
    }
}
